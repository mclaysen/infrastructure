app:
  dashboard_url: "https://pangolin.{{ domain }}"
  save_logs: true
  log_level: "info"
  telemetry:
    anonymous_usage: true

domains:
  domain1:
    base_domain: "{{ domain }}"
    cert_resolver: "letsencrypt"

server:
  secret: "{{ pangolin_tunnel_token }}"
  maxmind_db_path: "./config/GeoLite2/GeoLite2-Country.mmdb"
  maxmind_asn_path: "./config/GeoLite2/GeoLite2-ASN.mmdb"
  cors:
    origins: ["https://pangolin.{{ domain }}"]
    methods: ["GET", "POST", "PUT", "DELETE", "PATCH"]
    allowed_headers: ["X-CSRF-Token", "Content-Type"]
    credentials: false

gerbil:
  base_endpoint: "pangolin.{{ domain }}"
  start_port: 51820

flags:
  require_email_verification: false
  disable_signup_without_invite: true
  disable_user_create_org: true
  allow_raw_resources: true